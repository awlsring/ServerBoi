{
  "name": "@serverboi/ssdk",
  "version": "1.0.0",
  "description": "The package containing some utility functions",
  "main": "dist/index.js",
  "scripts": {
    "generate": "cd ../../models/api && gradle build && cd ./build/smithyprojections/api/typescript/typescript-ssdk-codegen",
    "extract": "cp -R ../../models/api/build/smithyprojections/api/typescript/typescript-ssdk-codegen/src/* ./src",
    "build": "yarn generate && yarn extract && yarn install && tsc --build"
  },
  "dependencies": {
    "tslib": "^2.3.1",
    "@aws-crypto/sha256-browser": "2.0.0",
    "@aws-crypto/sha256-js": "2.0.0",
    "@aws-sdk/config-resolver": "3.58.0",
    "@aws-sdk/fetch-http-handler": "3.58.0",
    "@aws-sdk/hash-node": "3.55.0",
    "@aws-sdk/invalid-dependency": "3.55.0",
    "@aws-sdk/middleware-content-length": "3.58.0",
    "@aws-sdk/middleware-retry": "3.58.0",
    "@aws-sdk/middleware-serde": "3.55.0",
    "@aws-sdk/middleware-stack": "3.55.0",
    "@aws-sdk/node-http-handler": "3.58.0",
    "@aws-sdk/protocol-http": "3.58.0",
    "@aws-sdk/smithy-client": "3.55.0",
    "@aws-sdk/types": "3.55.0",
    "@aws-sdk/url-parser": "3.55.0",
    "@aws-sdk/util-base64-browser": "3.58.0",
    "@aws-sdk/util-base64-node": "3.55.0",
    "@aws-sdk/util-body-length-browser": "3.55.0",
    "@aws-sdk/util-body-length-node": "3.55.0",
    "@aws-sdk/util-defaults-mode-browser": "3.55.0",
    "@aws-sdk/util-defaults-mode-node": "3.58.0",
    "@aws-sdk/util-utf8-browser": "3.55.0",
    "@aws-sdk/util-utf8-node": "3.55.0",
    "@aws-smithy/server-common": "1.0.0-alpha.5"
},
"devDependencies": {
    "@tsconfig/recommended": "1.0.1",
    "concurrently": "7.0.0",
    "downlevel-dts": "0.7.0",
    "rimraf": "^3.0.0",
    "typedoc": "^0.21.9",
    "@aws-sdk/service-client-documentation-generator": "3.58.0",
    "@types/jest": "latest",
    "@types/node": "^12.7.5"
}
}
