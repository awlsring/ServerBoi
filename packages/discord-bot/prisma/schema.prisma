// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TrackServer {
  id       String @unique
  startedAt DateTime @default(now())
  endedAt DateTime? @updatedAt
  name      String
  application String 
  address   String
  queryType String?
  queryAddress String?
  queryPort Int?
  ownerId String
}